{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BloodlineDNAS3Access",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket",
                "s3:GetObjectVersion",
                "s3:PutObjectAcl",
                "s3:GetObjectAcl",
                "s3:ListObjects"
            ],
            "Resource": [
                "arn:aws:s3:::bloodline-dna-files-v3",
                "arn:aws:s3:::bloodline-dna-files-v3/*"
            ]
        }
    ]
} 