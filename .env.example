# Copy this file to .env and fill values. Do NOT commit secrets.
# Example environment variables for the Leaf Shop project

# AWS / DynamoDB
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DYNAMODB_REGION=us-east-1
AWS_DYNAMODB_ENDPOINT=   # e.g. http://localhost:8000 for DynamoDB local
AWS_DYNAMODB_AUTO_CREATE=true

# Spring profile
SPRING_PROFILES_ACTIVE=local

# Application settings
SERVER_PORT=8080

# Database / other services
# - Add any other env vars your app expects here

# Security note: keep the real .env out of source control and rotate any keys accidentally exposed.

$BUCKET_NAME = "leaf-shop-frontend-prod"
$AWS_REGION = "ap-southeast-1"
