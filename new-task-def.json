{
	"family": "leaf-shop-lambda-task",
	"networkMode": "awsvpc",
	"requiresCompatibilities": ["FARGATE"],
	"cpu": "512",
	"memory": "1024",
	"executionRoleArn": "arn:aws:iam::083011581293:role/backend",
	"containerDefinitions": [
		{
			"name": "leaf-shop-lambda",
			"image": "083011581293.dkr.ecr.us-east-1.amazonaws.com/leaf-shop-lambda:latest",
			"essential": true,
			"portMappings": [
				{
					"containerPort": 8080,
					"hostPort": 8080,
					"protocol": "tcp"
				}
			],
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "/ecs/leaf-shop-lambda",
					"awslogs-region": "us-east-1",
					"awslogs-stream-prefix": "ecs"
				}
			}
		}
	]
}
